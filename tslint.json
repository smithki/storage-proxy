{
  "extends": "@ikscodes/tslint-config",
  "rules": {
    "function-name": false,
    "prettier": [true, {
      "printWidth": 120,
      "tabWidth": 2,
      "semi": true,
      "singleQuote": true,
      "trailingComma": "all",
      "bracketSpacing": true,
      "jsxBracketSameLine": true,
      "variable-name": {
        "options": [
          "allow-leading-underscore"
        ]
      }
    }]
  }
}
